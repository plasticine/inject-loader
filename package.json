  },
  "license": "MIT",
  "dependencies": {
    "loader-utils": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.14.0",
    "flow-bin": "^0.36.0",
    "jest": "^17.0.3",
